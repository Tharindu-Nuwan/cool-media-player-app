<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane fx:id="root" prefHeight="441.0" prefWidth="663.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep11.MainSceneController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <MediaView fx:id="mvMyVideo" fitHeight="200.0" fitWidth="200.0" />
                  <VBox alignment="BOTTOM_CENTER" prefHeight="482.0" prefWidth="664.0" StackPane.alignment="BOTTOM_CENTER">
                     <children>
                        <Slider fx:id="slrSeek" onMouseDragged="#slrSeekOnMouseDragged" onMousePressed="#slrSeekOnMousePressed">
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" />
                           </VBox.margin>
                        </Slider>
                        <HBox prefHeight="53.0" prefWidth="663.0">
                           <children>
                              <Button fx:id="btnChooseFile" mnemonicParsing="false" onAction="#btnChooseFileOnAction" text="Choose File">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnPlay" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnPlayOnAction" text="Play">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnPause" layoutX="103.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnPauseOnAction" text="Pause">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnStop" layoutX="149.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnStopOnAction" text="Stop">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnSlow" layoutX="206.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnSlowOnAction" text="&lt;&lt;">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnFast" layoutX="254.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnFastOnAction" text="&gt;&gt;">
                                 <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnBackward" layoutX="294.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnBackwardOnAction" text="-10s">
                                  <HBox.margin>
                                    <Insets left="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="btnForward" layoutX="334.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnForwardOnAction" text="+10s">
                                 <HBox.margin>
                                    <Insets left="5.0" right="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Slider fx:id="slrVolume" onMouseDragged="#slrVolumeOnMouseDragged" onMousePressed="#slrVolumeOnMousePressed" prefHeight="16.0" prefWidth="159.0">
                                 <HBox.margin>
                                    <Insets left="5.0" right="5.0" />
                                 </HBox.margin>
                              </Slider>
                           </children>
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
